{
  "schemaVersion": "1.0",
  "id": "integration-plex",
  "title": "Plex Integration",
  "description": "Starter integration package for Plex-related workspace integration assets.",
  "version": "0.1.0",
  "category": "integration",
  "source": {
    "kind": "github-release",
    "channel": "github",
    "fallbackOrder": ["github-release", "local"],
    "github": {
      "owner": "carapace-app",
      "repo": "carapace-integration-plex",
      "artifact": "integration-plex-${version}.zip"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carapace-app/carapace-integration-plex.git"
  },
  "compatibility": {
    "carapace": {
      "minVersion": "1.0.0"
    }
  },
  "dependencies": [
    {
      "id": "integration-media-providers",
      "version": ">=0.1.0"
    }
  ],
  "conflicts": [],
  "install": {
    "files": [
      {
        "source": "files/integrations/plex/README.md",
        "target": "integrations/plex/README.md"
      }
    ],
    "patches": [],
    "restart": {
      "recommended": true,
      "reason": "Integration registration refresh"
    }
  }
}